<div class="page-chat">
    <div class="sidebar">
        {{{logout}}}
        {{{createChat}}}
        {{{addUserToChat}}}
        {{{deleteUserFromChat}}}
        <div class="profile-link">
            <a href="/profile">Profile</a>
        </div>
        <div class="chats-search">
            {{{searchField}}}
        </div>
        <div class="chats-list">
            {{#each chatsList}}
                {{{this}}}
            {{/each}}
        </div>
    </div>
    <div class="chat-view">
        <div class="chat-view__header">
            <div class="chat-view__inner">
                <div class="chat-view__avatar">
                    <img src="{{chat.image_link}}" alt="avatar" width="40" height="40">
                </div>
                <div class="chat-view__name">
                    {{current_chat.title}}
                </div>
            </div>
            <div class="chat-view__menu">
                <a href="/profile">
                    <img src="https://img2.freepng.ru/20180425/qzq/kisspng-computer-icons-menu-bar-icon-design-hamburger-butt-5ae053e44abbe3.3712052415246509803061.jpg" alt="avatar" width="40" height="40">
                </a>
            </div>
        </div>
        <div class="chat-view__content">
            {{#each messages}}
                <div class="chat-content__item">
                    <div class="chat-content-inner">{{this.content}}</div>
                </div>
            {{/each}}
        </div>
        <div class="chat-view__footer">
            <div class="chat-view__form-message">
                <img class='chat-view__form-message-icon' src="data:image/png;base64," height="32px" width="32px" alt="attachment" aria-hidden="true">
                {{{messageField}}}
                {{{messageButton}}}
            </div>
        </div>
    </div>

    {{{modalCreateChat}}}
    {{{modalAddUserToChat}}}
    {{{modalDeleteUserFromChat}}}
</div>